.map_container {
  height: 100vh;
  width: 100%;
  position: relative;
}

.map {
  height: 100%;
  width: 100%;
}

.top {
  position: fixed;
  top: 20rpx;
  z-index: 1;
  background: #fff;
  color: #000;
  font-size: 24rpx;
  width: 95%;
  box-sizing: border-box;
  left: 2.5%;
  padding-left: 15rpx;
  height: 100rpx;
  padding-top: 10rpx;
  line-height: 40rpx;
  border-radius: 15rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.3);
}

.location {
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  bottom: 20rpx;
}

.findMarks {
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  right: 0;
  bottom: 140rpx;
}

.findMarks_bottom {
  bottom: 653rpx;
}

.searchLocation {
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  right: 0;
  bottom: 20rpx;
}

.bottom {
  bottom: 533rpx;
}

.modale {
  position: fixed;
  bottom: 10rpx;
  width: 98%;
  left: 1%;
  height: 530rpx;
  border-radius: 25rpx;
  background-color: #fff;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.3);
}

.sexRadio {
  width: 90rpx;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: relative;
}

.start, .between {
  height: 80rpx;
  line-height: 80rpx;
}

.dialogAge {
  position: fixed;
  bottom: 10rpx;
  width: 98%;
  left: 1%;
  height: 530rpx;
  border-radius: 25rpx;
  background-color: #fff;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.3);
  z-index: 11;
}

.dialogAge .contents {
  width: 100%;
  height: 350rpx;
  background-color: #fff;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;  
}

.left, .rights {
  height: 350rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  width: 50%;
  box-sizing: border-box;
}

.footers {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: space-around;
  flex-direction: row;
  z-index: 13;
  overflow: hidden;
  color: #000;
}

.footer-btn {
  width: 50%;
  text-align: center;
  height: 90rpx;
  color: #000;
  line-height: 90rpx;
  border-right: 2rpx solid #f7f7f7;
  border-top: 2rpx solid #f7f7f7;
  z-index: 12;
}

.footer-btn:last-child {
  border-right: none;
}
.ageTitle{
  width: 100%;
  display: flex;
 justify-content: space-around;
 line-height: 80rpx;
}
.h80{
  line-height: 80rpx;
}

.age {
  width: 100%;
  font-size: 28rpx;
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon {
  display: flex;
  justify-content: flex-start;
  margin-left: 15rpx;
  margin-right: 5rpx;
}

.active {
  background: url('data:image/png;base64,');
  background-size: 34rpx 34rpx;
  background-repeat: no-repeat;
  background-position: 10% 50%;
}

.ky {
  position: relative;
}

.center {
  display: flex;
  flex: 1;
  position: relative;
  align-items: center;
}

.content-range {
  width: 100%;
  height: 350rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}


.radioList {
  width: 80%;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  align-items: center;
}

.back {
  width: 120rpx;
  height: 60rpx;
  border: 2rpx solid #3cd5d1;
  color: #3cd5d1;
  font-size: 24rpx;
  border-radius: 8rpx;
  line-height: 60rpx;
  text-align: center;
  position: absolute;
  right: 0;
}

.title {
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-bottom: 2rpx solid #f7f7f7;
  margin-bottom: 10rpx;
}

.content {
  width: 95%;
  display: flex;
  margin: 0 auto;
}

.label {
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: left;
}

.label-right {
  width: 75%;
  font-size: 28rpx;
  word-break: keep-all;
  overflow: hidden;
  text-overflow: ellipsis;
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #ccc;
  border-radius: 4rpx;
  box-sizing: border-box;
  padding-left: 15rpx;
  margin-left: 25rpx;
}

.bg {
  background-color: #f4f4f4;
}

.borb {
  border-bottom: 1rpx solid #f7f7f7;
  height: 80rpx;
  line-height: 80rpx;
}

.kl {
  height: 80rpx;
  line-height: 80rpx;
}

input {
  width: 80%;
  border: 1rpx solid #e1e1e1;
  border-radius: 6rpx;
  font-size: 24rpx;
  box-sizing: border-box;
  padding-left: 8rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: left;
  margin-top: 25rpx;
}

.right {
  width: 75%;
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: keep-all;
  white-space: nowrap;
}

.content input:disabled {
  background-color: #f5f5f5 !important;
}

.none {
  display: none;
}

picker {
  width: 80%;
}

picker input {
  width: 100%;
}

radio {
  position: relative;
  bottom: 2rpx;
}

.radio-group {
  width: 80%;
  display: flex;
  align-items: center;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 15rpx;
}

.btn {
  background-color: #29d9d6;
  color: #fff;
  font-size: 28rpx;
  width: 96%;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 6rpx;
  text-align: center;
  margin-top: 20rpx;
}

.grayd {
  background-color: #e1e1e1;
  color: #ccc;
}

.bot {
  position: relative;
}

.icon1 {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  left: 15rpx;
}

.place_info {
  z-index: 1;
  width: 200rpx;
  height: 200rpx;
  background-color: #fff;
  color: #000;
  font-size: 24rpx;
}

radio .wx-radio-input {
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  border: none !important;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  height: 40rpx;
  width: 40rpx;
  font-size: 0;
  border: 2rpx solid #fff;
  background-color: #fff !important;
  color: #fff !important;
  transform: translate(-50%, -50%) scale(1);
  border-radius: 50%;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
