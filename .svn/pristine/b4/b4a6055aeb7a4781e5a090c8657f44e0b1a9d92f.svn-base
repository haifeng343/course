.addressForm{
  width: 100%;
  height: 100vh;
  background-color: #fff;
}
.item{
  width: 95%;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 5%;
  padding-right: 5%;
  box-sizing: border-box;
}
.between{
  border-bottom: 1rpx solid #f9f9f9;
}
.InputValue{
  width: 80%;
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  font-size: 28rpx;
}

.tagName{
  font-size:28rpx;
  font-weight: bold;
  position: absolute;
  left: -25%;
  top: -22rpx;
}
.mg{
  width: 100%;
  margin-left: 30rpx;
}
.start{
  width: 100%;
}
.adName{
  width: 120rpx;
  font-weight: bold;
  font-size: 28rpx;
  margin-left: 0rpx;
}

.label_frame {
  width:80%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.tagHome{
  font-size: 24rpx;
  border: 1rpx solid #E5E5E5;
  width: 120rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.sure{
  background-color: #29D9D6;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 70rpx;
  margin: 60rpx 5%;
  border-radius: 35rpx;
}
.active{
  color: #29D9D6;
  border-color: #29D9D6;
}





