.myTop {
  width: 95%;
  margin: 20rpx 2.5%;
  height: auto;
  background-color: #fff;
  padding: 20rpx 3%;
  box-sizing: border-box;
  border-radius: 15rpx;
  position: relative;
}

.setImg {
  position: absolute;
  right: 0;
  margin-right: 20rpx !important;
  top: 20rpx;
}

.ml2 {
  line-height: 45rpx;
  font-size: 32rpx;
}

.please {
  color: #000;
  padding: 5rpx 10rpx;
  margin: 0 5rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
}

.more1 {
  font-size: 24rpx;
  margin-left: 15rpx;
  color: #ccc;
}

.paypag {
  position: relative;
  width: 100%;
  justify-content: center;
  align-items: center;
  display: flex;
  margin-top: 20rpx;
}

.paypagItem {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  position: relative;
  border-right: 1rpx solid #e8e8e8;
  box-sizing: border-box;
}

.paypagItem:last-child {
  border: none;
}

.paypagItem .ml1 {
  font-size: 24rpx;
  color: #9a9a9a;
}

.avator {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.name {
  font-size: 32rpx;
  line-height: 45rpx;
  display: flex;
  justify-content: flex-start;
  text-align: left;
}

.share {
  font-size: 28rpx;
  color: #b1b1b1;
}

.code {
  font-size: 24rpx;
}

.align {
  display: flex;
  /* align-items: center; */
  flex-direction: column;
  text-align: left;
}

.List {
  width: 100%;
  height: auto;
}

.item {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}

.borb {
  border-bottom: 1rpx solid #f9f9f9;
}

.icon1 {
  width: 34rpx;
  height: 34rpx;
  margin: 0rpx 15rpx 0 0;
}

.vipBlock {
  width: 90%;
  height: 120rpx;
  background-color: #383E4C;
  color: #F6E1C3;
  font-size: 28rpx;
  margin: 0 auto;
  margin-bottom: -20rpx;
  border-radius: 15rpx;
  box-sizing: border-box;
  padding: 15rpx 5%;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.vipLeft {
  width: 70%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.vipTitle {
  font-size: 28rpx;
  line-height: 55rpx;
}

.vipCenter {
  width: auto;
  height: 56rpx;
  line-height: 56rpx;
  background-color: #F6E1C3;
  color: #000;
  font-size: 28rpx;
  padding: 0 30rpx;
  border-radius: 35rpx;
}

.swiper_container {
  height: 30rpx;
  line-height: 30rpx;
  width: 100%;
}

.swiper_item {
  font-size: 24rpx;
  color: #F6E1C3;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: left;
  /* letter-spacing: 2px; */
  /* padding-top: 20rpx; */
}
