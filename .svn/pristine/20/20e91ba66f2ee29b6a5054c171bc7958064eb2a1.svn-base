.navbar {
  display: flex;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1;
  width: 100%;
  height: 60rpx;
  flex-direction: row;
  text-align: center;
  color: #000;
  font-size: 28rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.navbar-item-active {
  /* transition: all 0.3s; */
  border-bottom: 4rpx solid #29d9d6;
  color: #000;
}

.navbar-item {
  flex: 1;
}
.List {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}

.item {
  width: 95%;
  margin: 20rpx auto 0;
  background-color: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 15rpx;
  display: flex;
  flex-direction: column;
}

.title {
  font-size: 28rpx;
  color: #5b5b5b;
}

.statusd {
  font-size: 28rpx;
}

.text {
  font-size: 28rpx;
  line-height: 40rpx;
}

.mtb {
  margin: 5rpx 0;
}

.code {
  width: 34rpx;
  height: 34rpx;
}

.footer {
  border-top: 1rpx solid #f4f4f4;
  padding: 15rpx;
  margin-top: 15rpx;
  color: #5b5b5b;
}

.codeImg {
  width: 350rpx;
  height: 350rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
}

.dialog-title {
  font-size: 32rpx;
  margin-bottom: 20rpx;
  text-align: center;
}

.dialog-code {
  font-size: 28rpx;
  text-align: center;
  margin-bottom: 25rpx;
}
