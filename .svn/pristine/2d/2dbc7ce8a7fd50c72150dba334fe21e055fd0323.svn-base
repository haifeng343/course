.dialogMask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 999;
  display: flex;
  justify-content: center;
  margin: auto;
  align-items: center;
  font-size: 28rpx;
}

.dialog-container {
  width: 90%;
  height: auto;
  background-color: #fff;
  border-radius: 15rpx;
  max-height: 80%;
  position: relative;
  z-index: 9999;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding-top: 0;
}

.dialog-title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #000;
}

.closeImg {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 25rpx;
  top: 25rpx;
}

.h40 {
  line-height: 40rpx;
}

.relative {
  position: relative;
}

.foot {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  top: 15rpx;
}

.dec {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx;
  color: #5b5b5b;
  border: 1rpx solid #f4f4f4;
  border-top: none;
  font-size: 24rpx;
}

.quanTitle {
  font-size: 28rpx;
}

.noWrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.startd {
  display: flex;
  justify-content: flex-start;
}

.quanImg {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}

.ml10 {
  margin-left: 10rpx;
}

.bb {
  color: #5b5b5b;
}

.quanContent {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 70%;
}

.none {
  display: none;
}

.dialog-content {
  font-size: 28rpx;
}

.red {
  color: red;
}

.dialog-content {
  width: 100%;
  height: auto;
  max-height: 800rpx;
  box-sizing: border-box;
  padding: 20rpx 5%;
}

.dialog-footer {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-around;
  border-top: 2rpx solid #f4f4f4;
  z-index: 9;
  background-color: #fff;
  border-bottom-right-radius: 15rpx;
  border-bottom-left-radius: 15rpx;
}

.dialog-btn {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  box-sizing: border-box;
  border-right: 2rpx solid #f4f4f4;
}

.dialog-btn:last-child {
  border: none;
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}

.zan-title {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.zan-content {
  font-size: 28rpx;
  line-height: 50rpx;
}

.icon1 {
  width: 34rpx;
  height: 34rpx;
  margin: 0rpx 15rpx 0 0;
}

.title {
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 20rpx;
}

.success {
  width: 100rpx;
  height: 100rpx;
}

.zan-dialog__container {
  padding: 40rpx 0;
}

.zan-dialog__mask {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 3;
  background: rgba(0, 0, 0, 0.4);
  display: none;
}

.zan-dialog__container {
  position: fixed;
  text-align: center;
  bottom: 400rpx;
  width: 650rpx;
  height: auto;
  margin-left: 50rpx;
  background: #fff;
  transform: translateY(300%);
  z-index: 12;
  border-radius: 20rpx;
}

.mb20 {
  margin-bottom: 20rpx;
}

.codeImg {
  width: 350rpx;
  height: 350rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
}

.css_appointment_frame {
  width: 80%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.btn {
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #29d9d5;
  border-radius: 15rpx;
  margin-top: 20rpx;
}
