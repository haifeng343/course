.searchTop{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  position: fixed;
  margin: auto 0;
  align-items: center;
  background-color: #ECECEC;
  z-index: 3;
}
.topInput {
  width: 80%;
  position: relative;
}
.clear{
  position: fixed;
  left: 74%;
  top: 26rpx;
}
.nohasLog{
  width: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  color: #6C6C6C;
  text-align: center;
  position: absolute;
  top: 80rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.ListItem{
  width: 100%;
  height: auto;
  border-radius: 15rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.titleTxt{
  width: 80%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: flex;
  align-items: center;
}
.distance{
  font-size:24rpx;
  color: #5b5b5b;
}
.tagList {
  width: 100%;
  overflow-x: scroll;
  display: flex;
  height: 40rpx;
  line-height: 40rpx;
  align-items: center;
  justify-content: flex-start;
  flex-direction: row;
}

.tagList::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.tagItem {
  word-break: keep-all;
  font-size: 24rpx;
  height: 36rpx;
  line-height: 36rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 8rpx;
  margin-right: 15rpx;
  margin-top: 0rpx;
  border-radius: 5rpx;
  box-sizing: border-box;
}
.search-placeholder {
  font-size: 28rpx;
}
.search {
  width: 95%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  margin-left: 5%;
  padding-right: 10%;
  padding-left: 10%;
  box-sizing: border-box;
  border: 1px solid #f2f2f2;
  background: #fff;
  border-radius: 40rpx;
  background-image: url('data:image/png;base64,');
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
  background-position: 5% 50%;
}
.searTxt{
  width: 20%;
  font-size: 28rpx;
  text-align: center;
  line-height: 60rpx;
}


.classList {
  position: absolute;
  top: 80rpx;
  left: 2.5%;
  width: 95%;
  height: auto;
  border-radius: 15rpx;
}

.classTitle {
  width: 100%;
  border-bottom: 1rpx solid #f4f4f4;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 2.5%;
  box-sizing: border-box;
}
.left{
  margin-right: 20rpx;
}
.classList .content-box{
  width: 95%;
  margin: 25rpx 2.5% 0 2.5%;
  padding-bottom: 25rpx;
  border-bottom: 1rpx solid #f4f4f4;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
}
.classList .content-box:last-child{
  border: none;
}
.leftImg {
  width: 172rpx;
  height: 220rpx;
  margin-right: 20rpx;
  border-radius: 15rpx;
}
.rightContent {
  width: 70%;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}
.ring {
  display: flex;
  justify-content: flex-start;
}
.ring:first-child{
  margin-right: 40rpx;
}
.courseTitle {
  width: 100%;
  font-size: 28rpx;
  font-weight: bold;
  line-height: 40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.name,.coursePeople {
  font-size: 24rpx;
  color: #5b5b5b;
  line-height: 36rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.centers{
  display: flex;
  justify-content: center;
  align-items: center;
}
.detail {
  display: flex;
}
.detailImg {
  width: 28rpx;
  height: 28rpx;
  margin-right: 6rpx;
}

