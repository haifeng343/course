.List{
  width: 100%;
  height: 100vh;
  background-color: #fff;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  box-sizing: border-box;
  padding-bottom: 120rpx;
}
.item{
  position: relative;
  width: 100%;
  margin-top: 10rpx;
  color: #fff;
  box-sizing: border-box;
}
.bgImg{
  width: 100%;
  height: 200rpx;
}
.noImg{
  margin: -120rpx auto 0;
}
.abso{
  width: 95%;
  position: absolute;
  top: 0;
  padding-left: 5%;
  box-sizing: border-box;
}
.success1{
  width: 100rpx;
  margin-top: 50rpx;
}
.le1{
  font-size: 32rpx;
  font-weight: 600;
  margin: 20rpx 0 10rpx;
}
.btnSubmit{
  width: 90%;
  margin: 60rpx 0 40rpx;
  margin-left: 5%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #fff;
  background-color: #29D9D6;
  border-radius: 10rpx;
}
.between{
  height: 200rpx;
}
.bankIcon{
  width: 100rpx;
  height: 100rpx;
  margin-top: 30rpx;
  margin-right: 30rpx;
}
.t1{
  font-size: 32rpx;
  margin-top: 20rpx;
}
.t2{
  font-size: 26rpx;
  margin-top: -5rpx;
}
.t3{
  font-size: 32rpx;
  letter-spacing: 2rpx;
  margin-top: 15rpx;
}
.delete{
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #f5f5f5;
  width: 100rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  padding: 0 20rpx;
  border-radius: 10rpx;
  margin-top: 70rpx;
  z-index: 9999;
  position: absolute;
  right: 5%;
  top: 0;
}


