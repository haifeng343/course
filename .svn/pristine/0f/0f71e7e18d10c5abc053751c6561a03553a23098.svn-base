.navbar {
  display: flex;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 199;
  width: 100%;
  height: 60rpx;
  flex-direction: row;
  justify-content: space-around;
  text-align: center;
  color: #000;
  font-size: 28rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.father {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  z-index: 21;
}

.center {
  color: #b5b5b5;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.navLeft {
  width: 92%;
  overflow-x: scroll;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.movie-content-wrapper {
  height: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}
scroll-view {
  height: 100%;
}
.navLi {
  width: 25%;
  height: 60rpx;
  box-sizing: border-box;
  margin-left: 40rpx;
  white-space: nowrap;
  overflow: hidden;
  padding-bottom: 8rpx;
}

.navLi:first-child {
  margin-left: 0;
}

.List {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  position: relative;
  margin-top: 60rpx;
}
.opacity {
  opacity: 0.6;
}

.item {
  width: 100%;
  margin: 20rpx auto 0;
  background-color: #fff;
  box-sizing: border-box;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;
}

.active {
  color: #3cd5cf;
  border-bottom: 4rpx solid #3cd5cf;
}

.topImg {
  width: 8%;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: -30rpx 0rpx 30rpx #fff;
}

.erweiImg {
  width: 34rpx;
  height: 34rpx;
}

.erFather {
  width: 80rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.dialog-title {
  font-size: 32rpx;
  margin-bottom: 20rpx;
  text-align: center;
}

.dialog-code {
  font-size: 28rpx;
  text-align: center;
  margin-bottom: 25rpx;
}

.codeImg {
  width: 350rpx;
  height: 350rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
}

.topImg image {
  width: 34rpx;
  height: 34rpx;
}

.allList {
  border-top: 1rpx solid #f4f4f4;
  width: 100%;
  height: auto;
  padding: 0 3%;
  position: relative;
  top: 60rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  z-index: 9;
}

.footerBtn {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 100rpx;
  background-color: #fff;
  color: #b5b5b5;
  position: fixed;
  bottom: 0;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 0 5%;
}

.allItem:nth-child(4n+1) {
  margin-left: 0;
}

.navbar-item-active {
  border-bottom: 4rpx solid #29d9d6;
  color: #000;
}

.allItem.on {
  border: none;
  background-color: #3cd5cf;
  color: #fff;
}

.allItem {
  width: 22%;
  margin-top: 20rpx;
  background-color: #f9f9f9;
  box-sizing: border-box;
  font-size: 24rpx;
  height: 50rpx;
  color: #5b5b5b;
  line-height: 50rpx;
  text-align: center;
  border-radius: 35rpx;
  margin-left: 4%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.logo {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}

.title {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 5%;
  border-bottom: 1rpx solid #f4f4f4;
  margin-bottom: 20rpx;
}

.storeName {
  font-size: 28rpx;
  width: 83%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.content {
  width: 90%;
  display: flex;
  margin-left: 5%;
  flex-direction: column;
}

.quan {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  height: auto;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-attachment: cover;
  /* box-shadow: 5rpx 0rpx 10rpx #f7f7f7; */
}

.up {
  width: 24rpx;
  height: 24rpx;
  margin-left: 10rpx;
}

.guoqi {
  position: absolute;
  width: 250rpx;
  text-align: center;
  background-color: #53e1de;
  color: #fff;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  transform: rotateZ(45deg);
  top: 23rpx;
  right: -78rpx;
}

.startd {
  display: flex;
  justify-content: flex-start;
  margin-bottom: 10rpx;
}

.quanImg {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}

.quanContent {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 70%;
}

.noWrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.quanTitle {
  font-size: 28rpx;
}

.ft28 {
  font-size: 24rpx;
}

.foot {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  top: 15rpx;
}

.mt25 {
  margin-top: 25rpx;
}

.btnd {
  width: 180rpx;
  height: 55rpx;
  line-height: 55rpx;
  border-radius: 45rpx;
  margin-right: 0;
  margin-top: 0;
}

.dec {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx;
  color: #5b5b5b;
  border: 1rpx solid #f4f4f4;
  border-top: none;
  font-size: 24rpx;
  /* box-shadow: 5rpx 5rpx 10rpx #f7f7f7; */
}

.none {
  display: none;
}
