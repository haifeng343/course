.navbar {
  display: flex;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 199;
  width: 100%;
  height: 60rpx;
  flex-direction: row;
  text-align: center;
  color: #000;
  font-size: 28rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.nameTitle{
  height: 45rpx;
  line-height: 45rpx;
}
.langqi {
  margin-top: -40rpx;
  z-index: 999;
}

.please text {
  color: #29d9d6;
  border: 1rpx solid #29d9d6;
  padding: 5rpx 10rpx;
  margin: 0 5rpx;
  border-radius: 8rpx;
}

.navbar-item {
  flex: 1;
}

.kld {
  width: 100%;
  display: flex;
  justify-content: flex-end;
  flex-direction: row;
}

.scorll {
  display: block;
  margin-top: 25rpx;
}

swiper {
  height: 100%;
}

.navbar-item-active {
  /* transition: all 0.3s; */
  border-bottom: 4rpx solid #29d9d6;
  color: #000;
}

.movie-content-wrapper {
  height: 100%;
  margin-top: 60rpx;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

scroll-view {
  height: 100%;
}

.orderList {
  width: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  display: flex;
  flex-direction: column;
}

.orderItem {
  width: 95%;
  height: auto;
  margin: 20rpx 2.5% 0 2.5%;
  box-sizing: border-box;
  padding: 20rpx 2.5%;
  background-color: #fff;
  border-radius: 15rpx;
  display: flex;
  flex-direction: column;
}

.orderItem:last-child {
  margin-bottom: 20rpx;
}

.title {
  font-size: 28rpx;
  margin-bottom: 15rpx;
}

.happening {
  font-size: 28rpx;
  width: 140rpx;
  text-align: right;
  word-break: keep-all;
  overflow: hidden;
  text-overflow: ellipsis;
}

.numbering {
  width: 100%;
  font-size: 28rpx;
}

.orderContent {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  line-height: 40rpx;
  border-bottom: 1rpx solid #f5f5f5;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}

.orderImg {
  width: 156rpx;
  height: 156rpx;
  margin-right: 15rpx;
  border-radius: 15rpx;
}

.hl {
  height: 55rpx;
  line-height: 55rpx;
}

.Right {
  width: 73%;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.name {
  width: 80%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  font-weight: bold;
  color: #000
}

.money {
  font-size: 28rpx;
  color: red;
}

.address {
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #5b5b5b;
}

.icon {
  width: 30rpx;
  height: 24rpx;
  margin-left: -5rpx;
}

.test {
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #5b5b5b;
  overflow: hidden;
  word-break: keep-all;
  text-overflow: ellipsis;
}

.tag {
  font-size: 28rpx;
}

.btn {
  font-size: 28rpx;
  height: 55rpx;
  line-height: 55rpx;
  border: 1rpx solid #5b5b5b;
  padding: 0rpx 30rpx;
  border-radius: 40rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.bottom {
  margin-top: 15rpx;
  display: flex;
  align-items: center;
  z-index: 1;
}

.zan-dialog__container {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  height: 380rpx;
}

.know {
  font-size: 28rpx;
  color: #9ff6f0;
  border-top: 1rpx solid #e0e0e0;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 28rpx;
}

.zan-dialog--show .zan-dialog__container {
  /* transform: translateY(0); */
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}

.zan-title {
  line-height: 100rpx;
  font-size: 32rpx;
  font-weight: 600;
}

.zan-content {
  font-size: 28rpx;
}
