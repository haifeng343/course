<movable-area style="height:{{windowHeight-statusBarHeight}}rpx;width:100%">
  <movable-view wx:if="{{obj.CustomerServiceInfo.PositionType == 1}}" direction="{{obj.CustomerServiceInfo.IsMove==true?'vertical':'none'}}" class='moveView1' y="{{obj.CustomerServiceInfo.PositionHight}}rpx">
    <button open-type='contact' class='contact contact_left'>客服</button>
  </movable-view>
  <movable-view wx:if="{{obj.CustomerServiceInfo.PositionType == 2}}" direction="{{obj.CustomerServiceInfo.IsMove==true?'vertical':'none'}}" class='moveView2 ' y="{{obj.CustomerServiceInfo.PositionHight}}rpx">
    <button open-type='contact' class='contact contact_right'>客服</button>
  </movable-view>
  <view class='topInput'>
    <input class='SearchInput' bindtap='searchTo' disabled placeholder='机构名称/课程'></input>
  </view>
  <view class='categoryTitle'>
    <view class='start w50' bindtap='allCategory'>
      <view class='txt'>{{txt}}</view>
      <image src="{{ImgType1==true?'../../images/xia.png':'../../images/up.png'}}" mode='aspectFill' class='icon1'></image>
    </view>
    <view class='start w50' bindtap='allSeller'>
      <view class='txt'>{{name}}</view>
      <image src="{{ImgType==true?'../../images/xia.png':'../../images/up.png'}}" mode='aspectFill' class='icon1'></image>
    </view>
  </view>


  <view wx:if="{{!ImgType}}">
    <view class='father' bindtap='collapse' catchtouchmove='true'></view>
    <view class='allList'>
      <view wx:for="{{typeList}}" wx:key wx:for-item="k" data-name="{{k.Name}}" data-Id="{{k.Id}}" class="allItem {{showId == k.Id ? 'on' : ''}}" bindtap='changeType'>{{k.Name}}</view>
    </view>
  </view>

  <view wx:if="{{!ImgType1}}" catchtouchmove='true'>
    <view class='father' bindtap='collapse' catchtouchmove='true'></view>
    <view class='adsList'>
      <scroll-view scroll-y="true" class='adsLeft' scroll-with-animation="true">
        <view wx:for="{{cityList}}" wx:key wx:for-item="m" data-index="{{index}}" class="adsLeftItem {{showId1 == index ? 'active' : ''}}" data-districtname="{{m.District}}" bindtap='changeType1'>{{m.District}}</view>
      </scroll-view>
      <scroll-view scroll-y="true" class='adsRight' scroll-with-animation="true" wx:if="{{sqList.length>0}}">
        <view wx:for="{{sqList}}" wx:key wx:for-item="l" data-TradingareaId="{{l.TradingareaId}}" data-name="{{l.TradingareaName}}" bindtap='checkTrading' class="adsRightItem {{showId2 == l.TradingareaId ? 'active' : ''}}">{{l.TradingareaName}}</view>
      </scroll-view>
      <scroll-view scroll-y="true" class='adsRight' scroll-with-animation="true" wx:else>
        <view class='adsRightItem'>暂无商圈，敬请期待</view>
      </scroll-view>
    </view>
  </view>

  <view wx:if="{{List.length>0}}">
    <view class='classList'>
      <view class='ListItem' wx:for="{{List}}" wx:key>
        <view class='classTitle between'>
          <view class='titleTxt'>{{item.TradingareaName}}</view>
          <view class='distance' style='font-size:24rpx' wx:if="{{item.Distance<1}}">{{item.Distance*1000}}m</view>
          <view class='distance' style='font-size:24rpx' wx:else>{{item.Distance}}km</view>
        </view>
        <view class='content-box' wx:for="{{item.ItemList}}" wx:key wx:for-item="b" data-type="{{item.SheetModel}}" data-Id="{{item.SheetId}}" data-storeid="{{b.StoreId+'NUV'+b.GroupId}}" data-relId="{{b.RelId}}" bindtap='cheoose'>
          <image src='{{b.ItemCoverImg}}' mode='aspectFill' class='leftImg'></image>
          <view class='rightContent' data-Id="{{b.RelId}}" wx:key bindtap='courseDetail'>
            <view class='between relative'>
              <view class='courseTitle'>{{b.ItemName}}</view>
            </view>
            <view class='detail'>
              <view class='centers'>
                <image class='detailImg' src='../../images/house.png' mode='widthFix' lazy-load='true'></image>
              </view>
              <view class='name'>{{b.SellerName}}</view>
            </view>
            <view class='detail around'>
              <view class='centers'>
                <image class='detailImg' src='../../images/ad1.png' mode='widthFix' lazy-load='true'></image>
              </view>
              <view class='name address'>{{b.StoreAddress}}</view>
            </view>
            <view class='detail'>
              <view class='ring'>
                <view class='centers'>
                  <image class='detailImg' src='../../images/couse.png' mode='widthFix' lazy-load='true'></image>
                </view>
                <view class='name'>共有{{b.CoursesNumber}}节课</view>
              </view>
              <view class='ring'>
                <view class='centers'>
                  <image class='detailImg' src='../../images/time.png' mode='widthFix' lazy-load='true'></image>
                </view>
                <view class='name'>{{b.SingleTime}}min/课时</view>
              </view>
            </view>
            <view class='tagList' wx:if="{{b.TagList.length>0}}">
              <view class='tagItem' wx:for="{{b.TagList}}" wx:key wx:for-item="l" style='background:{{l.TagBgColor}};color:{{l.TagFontColor}};border:2rpx solid {{l.BorColor}}'>{{l.TagName}}</view>
            </view>
            <view class='coursePeople'>{{b.SoldCount}}人已参与 | {{b.RemainCount}}剩余名额</view>
          </view>
        </view>
      </view>
    </view>
  </view>
  <view wx:else>
    <view class='nohasLog'>
      <image class='noImg' src='../../images/zanwu.png' mode='widthFix' lazy-load='true'></image>
      <view>暂无查询结果</view>
    </view>
  </view>


  <view wx:for="{{popList}}" wx:key wx:for-item="item1">
    <view wx:if="{{item1.pop==true}}">
      <view class='fatherPop' catchtouchmove='true'></view>
      <view class='poFixed'>
        <cover-view class='flex-center'>
          <cover-view class='flexBetween' wx:if="{{item1.ShowClose}}">
            <cover-image src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10000?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10000?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
            <cover-image src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10001?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10001?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
            <cover-image src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10002?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10002?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
          </cover-view>
          <cover-view class='flexBetween centered'>
            <cover-image wx:if="{{item1.ShowClose}}" src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10003?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10003?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
            <cover-image src='{{item1.ImgUrlShow}}' style="width:{{windowWidth*item1.ShowImgWidth*1.0/100}}rpx;" data-actiontype="{{item1.ActionType}}" data-actionparams="{{item1.ActionParams}}" mode='aspectFill' data-executeparams="{{item1.ExecuteParams}}" data-popId="{{item1.Id}}"
              bindtap='popclick' data-index="{{index}}" class='hongbao'></cover-image>
            <cover-image wx:if="{{item1.ClosePositionX}}<10000 && item1.ShowClose" src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX<10000?'shutDown':''}}" mode='aspectFill' class='shut' style='top:{{item1.ClosePositionY}}rpx;left:{{item1.ClosePositionX}}rpx;width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
            <cover-image wx:if="{{item1.ShowClose}}" src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10004?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10004?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
          </cover-view>
          <cover-view class='flexBetween' wx:if="{{item1.ShowClose}}">
            <cover-image src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10005?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10005?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
            <cover-image src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10006?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10006?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
            <cover-image src='{{item1.CloseImgUrlShow}}' data-index="{{index}}" bindtap="{{item1.ClosePositionX==10007?'shutDown':''}}" mode='aspectFill' class="shuts {{item1.ClosePositionX!=10007?'IsHide':''}}" style='width:{{item1.CloseWidth}}rpx;height:{{item1.CloseHeight}}rpx'></cover-image>
          </cover-view>
        </cover-view>
      </view>
    </view>
  </view>
</movable-area>