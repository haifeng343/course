page{
  background-color: #fff;
}
.List {
  width: 100%;
  height: 88%;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
}

.name {
  border-top: 1rpx solid #f4f4f4;
  font-size: 32rpx;
  padding-top: 20rpx;
  margin: 20rpx 0 0;
  box-sizing: border-box;
  padding-left: 5%;
  text-align: left;
}

.item {
  width: 90%;
  height: auto;
  margin: 0 5%;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #f4f4f4;
  display: flex;
  justify-content: space-between;
}

.item:last-child {
  border: none;
}

.itemName {
  font-size: 28rpx;
  color: #000;
}

.itemTime {
  font-size: 24rpx;
  color: #b1b1b1;
}

.itemMoney {
  font-size: 28rpx;
  color: red;
  display: flex;
  align-items: center;
}