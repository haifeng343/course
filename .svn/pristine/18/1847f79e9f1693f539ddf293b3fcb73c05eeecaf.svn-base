.Bill {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  border-top: 1rpx solid #f9f9f9;
  border-bottom: 1rpx solid #f9f9f9;
}

.List {
  width: 100%;
  height: auto;
}

.item {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 140rpx;
  line-height: 140rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}

.icon1 {
  width: 40rpx;
  height: 34rpx;
  margin: 0 15rpx 0 0;
}

.go {
  margin: 0 0 0 15rpx;
}

.align {
  display: flex;
  align-items: center;
}

.bankIcon {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 15rpx;
}

.bankText {
  line-height: 40rpx;
}

.bankTitle {
  font-size: 28rpx;
  font-weight: 600;
}

.bankTxt {
  font-size: 26rpx;
  color: #5b5b5b;
}

.bankRight {
  margin-top: 55rpx;
}
.mTxt{
  font-size: 28rpx;
}
.item1 {
  width: 100%;
  height: 160rpx;
  box-sizing: border-box;
  padding: 0 5%;
  background-color: #fff;
}

.bor {
  border-top: 1rpx solid #f9f9f9;
}

.withdrawTxt {
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #5b5b5b;
}

.moneyTxt {
  font-size: 28rpx;
  color: #3dd6d1;
}
.btnd{
  width: 150rpx;
  margin-top: 0;
  border-radius: 40rpx;
}

.btn1 {
  width: 95%;
  display: flex;
  align-items: center;
  margin: 0 auto;
  border-radius: 15rpx;
}

.start {
  position: relative;
}

.clears {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  left: 32%;
  z-index: 55;
}

.end {
  position: absolute;
  right: 0;
}

.money {
  width: 40%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  padding-right: 15%;
}

.item2 {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #5b5b5b;
  box-sizing: border-box;
  padding: 0 5%;
  background-color: #fff;
  margin-bottom: 60rpx;
}
.startd{
  display: flex;
  justify-content: flex-start;
}
.centers{
  height: 34rpx;
  display: flex;
  align-items: center;
}
.g1 {
  color:#fff!important;
  background-image: linear-gradient(180deg, #cacaca ,#cacaca)!important;
}

.le {
  font-size: 50rpx;
  font-weight: 600;
  margin-top: 10rpx;
}

.btnSubmit {
  width: 95%;
  margin: 60rpx 0 40rpx;
  margin-left: 2.5%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #fff;
  background-color: #29d9d6;
  border-radius: 10rpx;
}

.success1 {
  width: 100rpx;
  height: 100rpx;
  margin-top: 40rpx;
}

.le1 {
  font-size: 32rpx;
  font-weight: 600;
}
.List1{
  width: 100%;
  height: auto;
  background-color: #fff;
}
.title{
  width: 100%;
  box-sizing: border-box;
  margin-left: 5%;
  padding: 25rpx 0;
  border-bottom: 1rpx solid #f4f4f4;
  display: flex;
  flex-direction: column;
}
.content{
  width:100%;
  box-sizing: border-box;
  padding: 25rpx 5%;
  display: flex;
  justify-content: flex-end;
}
.ml40{
  margin-left: 40rpx;
}
.txt1{
  font-size: 24rpx;
}
.titleText{
  width: 85%;
  font-size: 28rpx;
  font-weight: 600;
}
.text1{
  border: 2rpx solid #FE9B84;
  line-height: 70rpx;
  margin-right: 40rpx;
  font-size: 28rpx;
  padding: 0 40rpx;
  border-radius: 40rpx;
  color: #FE9B84;
}
.btnd1{
  margin: 0;
  width: 240rpx;
  border: none;
  color: #fff;
  background-image: linear-gradient(180deg, #FF393A ,#FF5E38);
}
.icon2{
  width: 34rpx;
  height: 34rpx;
  margin-right: 15rpx;
}

.nohave {
  width: 450rpx;
  display: flex;
  justify-content: center;
  margin: 0 auto;
  margin-top: 150rpx;
  border: 2rpx solid red;
  border-radius: 40rpx;
  text-align: center;
  box-sizing: border-box;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
}
