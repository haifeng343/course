.subsidy-top {
  width: 90%;
  height: auto;
  color: #fff;
  border-radius: 15rpx;
  background-image: linear-gradient(#fb7262, #f96758);
  margin: 20rpx auto;
  display: flex;
  justify-content: flex-start;
  position: relative;
  box-sizing: border-box;
  padding: 0 5%;
}
.klms{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
}
.quanTitle {
  width: 80%;
}
.jichu{
  position: absolute;
  right: 7%;
}
.subsidy-top-text {
  font-size: 32rpx;
  box-sizing: border-box;
  height: 150rpx;
  display: flex;
  align-items: center;
}

.subsidy-top-text span {
  font-size: 36rpx;
  padding: 0 10rpx;
}

.more {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.dialog-content{
  height: 800rpx;
}
.allowance{
  font-size: 22rpx;
  display: flex;
  align-items: center;
  position: relative;
  bottom: -5rpx;
  left: 20rpx;
}
.List {
  width: 100%;
  height: auto;
}

.item {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}

.icon1 {
  width: 34rpx;
  height: 34rpx;
  margin: 0rpx 15rpx 0 0;
}

radio .wx-radio-input {
  height: 48rpx;
  width: 48rpx;
  border-radius: 50%;
  border: none;
  color: #fff;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  height: 48rpx;
  width: 48rpx;
  border-radius: 50%;
  color: transparent !important;
  transform: translate(-50%, -50%) scale(1);
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

