page {
  background-color: #fff;
}
.vipTop{
  padding: 30rpx;
}
.lastItem {
  margin-bottom: 20rpx;
}
.vipLog {
  top: 50rpx;
}
.pay {
  height: 100rpx;
  z-index: 2;
}
.zanwu{
  color: #5b5b5b;
  height: 100%;
  display: flex;
  align-items: center;
  text-align: center;
  margin:auto;
}
.vipText {
  width: 60%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.btn {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 45rpx;
  font-size: 28rpx;
  color: #f8e1c3;
  margin: 15rpx auto;
  text-align: center;
  background-color: #333844;
}

.List {
  width: 100%;
  height: 88%;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
}

.name {
  border-top: 1rpx solid #f4f4f4;
  font-size: 32rpx;
  padding-top: 20rpx;
  margin: 20rpx 0 0;
  box-sizing: border-box;
  padding-left: 5%;
  text-align: left;
}

.item {
  width: 90%;
  height: auto;
  margin: 0 5%;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #f4f4f4;
  display: flex;
  justify-content: space-between;
}

.item:last-child {
  border: none;
}

.itemName {
  font-size: 28rpx;
  color: #000;
}

.itemTime {
  font-size: 24rpx;
  color: #b1b1b1;
}

.itemMoney {
  font-size: 28rpx;
  color: red;
  display: flex;
  align-items: center;
}
.zan-dialog{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background-color: rgba(0, 0, 0, 0.4);
}
.boldFont{
  font-size: 32rpx;
  padding: 0 2rpx;
}
.zan-container{
  width: 90%;
  height: 260rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  background-color: #fff;
  border-radius: 20rpx;
  z-index: 999;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.zan-container1{
  height: 340rpx;
  padding-top: 20rpx;
}
.success1 {
  width: 100rpx;
  height: 100rpx;
  margin-top: 40rpx;
  margin: 0 auto;
}
.btn1{
  width: 90%;
  margin-top: 20rpx;
}