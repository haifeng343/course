page{
  background-color: #fff;
}
.searchTop{
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}
.topInput {
  width: 80%;
  position: relative;
}
.mt30{
  margin-top: 30rpx;
}

.search-placeholder {
  font-size: 28rpx;
}
.search {
  width: 95%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  margin-left: 5%;
  padding-right: 10%;
  padding-left: 10%;
  box-sizing: border-box;
  border: 1px solid #f2f2f2;
  background: #F6F6F6;
  border-radius: 40rpx;
  background-image: url('data:image/png;base64,');
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
  background-position: 5% 50%;
}
.searTxt{
  width: 20%;
  font-size: 28rpx;
  text-align: center;
  line-height: 60rpx;
}
.searchHistory,.historyList{
  width: 100%;
  height:auto;
  padding: 0 5%;
  box-sizing: border-box;
}
.histitle{
  font-size: 30rpx;
  margin-bottom: 10rpx;
}
.delete{
  width: 45rpx;
  height: 45rpx;
}
.historyList{
  padding: 0;
  max-height: 450rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.historyItem{
  font-size: 28rpx;
  padding: 5rpx 15rpx;
  border-radius: 4rpx;
  background-color: #F1F1F1;
  color: #919191;
  margin-right: 30rpx;
  margin-top: 25rpx;
}
.mt40{
  margin-top: 40rpx;
}




.groupList {
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  padding: 0 2.5%;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.groupItem {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
  flex-direction: row;
  margin-top: 60rpx;
}

.itemImg {
  width: 35%;
  overflow: hidden;
  margin-right: 15rpx;
}

.leftImg {
  width: 250rpx;
  border-radius: 10rpx;
}

.itemRight {
  width: 64%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.title {
  font-size: 32rpx;
  font-weight: 600;
}

.ad1 {
  width: 30rpx;
  height: 30rpx;
  position: relative;
  top: 13rpx;
}

.addr {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  color: #5b5b5b;
}

.distance {
  color: #5b5b5b;
}

.time {
  font-size: 28rpx;
  color: #b1b1b1;
}

.tagList {
  width: 100%;
  overflow-x: scroll;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}

.tagList::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.tagItem {
  word-break: keep-all;
  font-size: 24rpx;
  padding: 2rpx 8rpx;
  margin-right: 15rpx;
  margin-top: 0rpx;
  box-sizing: border-box;
}

.bottom {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  margin-top: 15rpx;
}

.bottom-left {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}

.money {
  font-size: 34rpx;
  color: #ed1c1d;
}

.integral {
  font-size: 24rpx;
  color: #fe7860;
  border: 1rpx solid #fe7860;
  border-radius: 6rpx;
  padding: 2rpx 6rpx;
  height: 34rpx;
  margin-left: 15rpx;
  position: relative;
  top: 3rpx;
}

.bottom-right {
  display: flex;
  justify-content: flex-end;
  flex-direction: row;
  color: #aeaeae;
}

.go {
  width: 30rpx;
  position: relative;
  top: 5rpx;
}

/* 自定义导航栏开始 */

.custom {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 90rpx;
  line-height: 90rpx;
  /* background: #c00; */
  z-index: 999;
  background: #fff;
  display: flex;
  align-items: center;
}

.navTop {
  float: left;
  font-weight: 600;
  width: 400rpx;
  overflow: hidden;
  box-sizing: border-box;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: flex-start;
}

.abs {
  width: 300rpx;
  font-size: 32rpx;
  word-break: keep-all;
  display: flex;
  height: 58rpx;
  overflow: hidden;
  line-height: 58rpx;
  flex-wrap: nowrap;
}

.navTop::after {
  content: "";
  position: absolute;
  width: 400rpx;
  height: 58rpx;
  top: 0;
  left: 0;
  box-shadow: inset -20rpx 0px 20rpx #fff;
  z-index: 1;
}

.navImg {
  width: 30rpx;
  height: 36rpx;
  display: flex;
  align-items: center;
  margin-right: 5rpx;
  line-height: 58rpx;
}

.groupList {
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  padding: 0 2.5%;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.groupItem {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
  flex-direction: row;
  margin-top: 40rpx;
  border-bottom: 1rpx solid #f7f7f7;
  padding-bottom:15rpx;
  margin-bottom:-15rpx;
}
.groupItem:last-child{
  border: none;
}

.itemImg {
  width: 32%;
  overflow: hidden;
  margin-right: 15rpx;
}

.h30 {
  line-height: 30rpx;
}

.h40 {
  line-height: 46rpx;
  display: flex;
  align-items: center;
}

.h50 {
  line-height: 50rpx;
}

.leftImg {
  width: 220rpx;
  height: 220rpx;
  border-radius: 15rpx;
}

.tagItem {
  border-radius: 6rpx;
}

.itemRight {
  width: 67%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.title {
  font-size: 32rpx;
  font-weight: 600;
}

.ad1 {
  width: 35rpx;
  height: 28rpx;
}

.addr {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  font-size: 30rpx;
  color: #5b5b5b;
  width: 80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:nowrap;
}
.address{
  width: 90%;
  overflow: hidden;
  text-overflow: ellipsis;
}



