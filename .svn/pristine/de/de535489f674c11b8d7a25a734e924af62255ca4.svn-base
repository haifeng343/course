<view class='mTop'>
  <image class='mTopImg' src='../../images/mm1.png' mode='widthFix' lazy-load='true'></image>
  <view class='column mt10'>
    <view class='Balance'>我的余额</view>
    <view class='b1'>
      <text class='money'>￥{{money}}</text>
    </view>
  </view>
    <view class='rule' bindtap='balanceLog'>余额变更记录</view>
</view>
<view class='List mt20'>
  <view class='item mt20' bindtap='Bill'>
    <view class='start'>
      <image class='icon1' src='../../images/m1.png' mode='widthFix' lazy-load='true'></image>
      <view>账单</view>
    </view>
    <view class='start'>
      <image class='go' src='../../images/go.png' mode='widthFix' lazy-load='true'></image>
    </view>
  </view>
  <view class='item mt20 borb' bindtap='withdraw'>
    <view class='start'>
      <image class='icon1' src='../../images/m2.png' mode='widthFix' lazy-load='true'></image>
      <view>提现</view>
    </view>
    <view class='start'>
      <image class='go' src='../../images/go.png' mode='widthFix' lazy-load='true'></image>
    </view>
  </view>
  <view class='item' bindtap='cashBack'>
    <view class='start'>
      <image class='icon1' src='../../images/m3.png' mode='widthFix' lazy-load='true'></image>
      <view>返现记录</view>
    </view>
    <view class='start'>
      <image class='go' src='../../images/go.png' mode='widthFix' lazy-load='true'></image>
    </view>
  </view>
  <view class='item mt20' bindtap='bankList'>
    <view class='start'>
      <image class='icon1' src='../../images/m4.png' mode='widthFix' lazy-load='true'></image>
      <view>银行卡</view>
    </view>
    <view class='start'>
      <image class='go' src='../../images/go.png' mode='widthFix' lazy-load='true'></image>
    </view>
  </view>
</view>
