<view class='vipTop mt20'>
  <view class='vipUserInfo'>
    <image src='../../images/userIcon.png' mode='aspectFill' class='userImg'></image>
    <view class='vipText'>
      <view class='vipName'>合伙基金 (元): 2290.00</view>
      <!-- <view>您好，合伙人</view> -->
      <view>成为合伙人，享更多收益</view>
    </view>
  </view>
  <view class='vipLog' bindtap='bindAbout'>
    <view class='vipLogText'>合伙权益</view>
    <image src='../../images/vipIcon.png' mode='aspectFill' class='vipIcon'></image>
  </view>
</view>

<view class='List'>
  <view class='name'>基金增长记录</view>
  <view class='item'>
    <view class='column'>
      <view class='itemName'>用户增加都是</view>
      <view class='itemTime'>2019-12-12</view>
    </view>
    <view class='itemMoney'>+￥16</view>
  </view>
  <view class='item'>
    <view class='column'>
      <view class='itemName'>用户增加都是</view>
      <view class='itemTime'>2019-12-12</view>
    </view>
    <view class='itemMoney'>+￥16</view>
  </view>
</view>
