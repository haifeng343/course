<view class='lastItem'></view>
<view class='pay'>
  <button class='btn' open-type='share' bindtap='transpond'>转入余额</button>
</view>

<!-- 转入余额 -->
<view class="zan-dialog" hidden='{{!showLog}}' catchtouchmove="ture">
  <view class="zan-dialog__mask"/>
  <view class="zan-container">
    <view class='zan-title'>转入余额</view>
    <view class='title'>确认转入<span class="boldFont">￥28.00</span>到钱包吗？</view>
    <button class='btn1' bindtap='closeded'>知道了</button>
  </view>
</view>
<!-- 转入余额成功 -->
<view class="zan-dialog" hidden='{{!showSuccess}}' catchtouchmove="ture">
  <view class="zan-dialog__mask" bindtap="toggleDialog" />
  <view class="zan-container zan-container1">
    <image src='../../images/success1.png' class='success1' mode='aspectFill'></image>
    <view class='zan-title'>转入余额成功</view>
    <view class='title'>请至您的钱包查看</view>
    <view class='know' bindtap='closeded'>知道了</view>
  </view>
</view>