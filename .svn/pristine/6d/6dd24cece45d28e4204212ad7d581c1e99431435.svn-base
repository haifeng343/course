<view class='vipTop'>
  <view class='vipUserInfo'>
    <image src='../../images/userIcon.png' mode='aspectFill' class='userImg'></image>
    <view class='vipText'>
      <view class='vipName' wx:if="{{vip==true}}">账户名
        <image src='../../images/vip.png' mode='aspectFill' class='vipIcon1'></image>
      </view>
      <view class='vipName' wx:else>账户名</view>
      <view>成为会员享4大权益</view>
      <!-- <view>有效期至 2019-10-23</view> -->
    </view>
  </view>
  <view class='vipLog'>
    <view class='vipLogText' bindtap='bindRecharge'>充值记录</view>
    <image src='../../images/vipIcon.png' mode='aspectFill' class='vipIcon'></image>
  </view>
  <view class='vipContent'>
    <view class='vipItem'>
      <view class='vipNumber'>{{price1}}</view>
      <view>累计节省 (元)</view>
    </view>
    <view class='vipItem' bindtap='bindFund'>
      <view class='vipNumber'>{{price2}}</view>
      <view>合伙基金 (元)</view>
    </view>
  </view>
</view>

<view class='vipCard'>
  <view class="vipCardItem {{showId==item.id?'active':''}}" wx:for="{{cardList}}" wx:key data-name="{{item.name}}" data-id="{{item.id}}" bindtap='clickItem'>
    <view class='vipCardName'>{{item.name}}</view>
    <view class='vipCardMoney'>
      <span class="fuhao">￥</span>{{item.price}}</view>
    <view class='vipCardOldMoney'>￥{{item.oldPrice}}</view>
  </view>
</view>
<view class='vipInterests'>会员权益</view>
<view class='vipList'>
  <view class='vipInterItem' wx:for="{{vipList}}" wx:key wx:for-item="item1">
    <view class='column'>
      <view class='between'>
        <view class='start w75'>
          <image src='{{item1.icon}}' mode='aspectFill' class='vipItenIcon'></image>
          <view class='vipItemTitlte'>{{item1.name}}</view>
        </view>
        <view class='vipBtn' data-decname="{{item1.decTitle}}" data-dec="{{item1.dec}}" bindtap='showModal'>{{item1.buttonName}}</view>
      </view>
    </view>
    <view class='vipItemContent'>{{item1.text}}</view>
  </view>
</view>
<view class='lastItem'></view>
<view class='pay'>
  <view class='btn'>开通会员 ({{showName}})</view>
  <!-- <view class='btn'>立即续费 ({{showName}})</view> -->
</view>