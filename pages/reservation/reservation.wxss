.reserContainer{
  width: 95%;
  height: auto;
  margin: 20rpx auto;
  background-color: #fff;
  border-radius: 15rpx;
  display: flex;
  flex-direction: column;
}
.reserItem{
  display: flex;
  width: 95%;
  box-sizing: border-box;
  padding: 20rpx 0;
  margin-left: 5%;
  border-bottom: 1rpx solid #f4f4f4;
  flex-direction: row;
  position: relative;
}
.reserItem:last-child{
  border: none;
}
.reserItem label{
  font-size: 28rpx;
  color: #5b5b5b;
}
.reserItem .Itemtext{
  font-size: 28rpx;
  margin-left: 15rpx;
}
.rightTIme{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  right: 2.5%;
}
.placeholder{
  color: #D0D0D0;
}
.propList{
  width: 95%;
  box-sizing: border-box;
  padding: 20rpx 5%;
  height: auto;
  background-color: #fff;
  margin: 20rpx auto;
  border-radius: 15rpx;
}
.prompt{
  border-radius: 15rpx;
}
.prompTitle {
  font-size: 28rpx;
  font-weight: bold;
}

.promptText {
  font-size: 24rpx;
  line-height: 36rpx;
  margin-top: 10rpx;
}

.title{
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 20rpx;
}
.success{
  width: 100rpx;
  height: 100rpx;
}
.zan-dialog__container{
  padding: 40rpx 0;
}
.beter{
  display: flex;
  flex: 1;
  justify-content: space-between;
}
.rightTIme{
  width: 34rpx;
  height: 34rpx;
}
.btn{
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #29D9D5;
  border-radius: 15rpx;
  margin-top: 20rpx;
}









