/* 自定义导航栏开始 */

.custom {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 90rpx;
  /* background: #c00; */
  z-index: 999;
  background: #fff;
}

.navTop {
  float: left;
  margin-top: 16rpx;
  font-weight: 600;
  width: 320rpx;
  height: 58rpx;
  line-height: 58rpx;
  overflow: hidden;
  box-sizing: border-box;
  padding-left: 60rpx;
  position: relative;
  top: 5rpx;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: flex-start;
}
.abs{
  width: 300rpx;
  font-size: 32rpx;
}
.navTop::after{
  content: "";
  position: absolute;
  width: 320rpx;
  height: 58rpx;
  top: 0;
  left: 0;
  box-shadow:inset -20rpx 0px 20rpx #fff;
  z-index: 1;
}
.navImg {
  width: 45rpx;
  height: 28rpx;
  position: relative;
  top: 11rpx;
}

/* 自定义导航栏结束 */

.topInput {
  background: #fff;
}

.search {
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
  margin-left: 5%;
  border: 1px solid #f2f2f2;
  background: #f2f2f2;
  border-radius: 40rpx;
  background-image: url('data:image/png;base64,');
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
  background-position: 37% 50%;
}

.banner {
  width: 100%;
  height: 300rpx;
  padding-top: 30rpx;
  padding-left: 2.5%;
  box-sizing: border-box;
  position: relative;
  z-index: 0;
  background: #fff;
}

swiper {
  height: 100%;
}

swiper-item {
  text-align: center;
}

swiper-item image {
  width: 690rpx;
  height: 240rpx;
  object-fit: cover;
  border-radius: 30rpx;
}

.box {
  width: 100%;
  height: 100%;
  display: inline-block;
  box-sizing: border-box;
}

.indicaor-color {
  background-color: #fff;
}

.dots {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 20rpx;
  flex-direction: row;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 10rpx;
  z-index: 1;
}

/*未选中时的小圆点样式 */

.dot {
  width: 10rpx;
  height: 10rpx;
  border-radius: 14rpx;
  margin-right: 20rpx;
  background-color: #fff;
}

/*选中以后的小圆点样式  */

.active {
  width: 30rpx;
  height: 10rpx;
  background-color: #fff;
}

.groupList {
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  padding: 0 2.5%;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.groupItem {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
  flex-direction: row;
  margin-top: 60rpx;
}

.itemImg {
  width: 35%;
  overflow: hidden;
  margin-right: 15rpx;
}

.leftImg {
  width: 250rpx;
  height: 250rpx;
  border-radius: 10rpx;
}

.itemRight {
  width: 64%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.title {
  font-size: 32rpx;
  font-weight: 600;
}

.ad1 {
  width: 35rpx;
  height: 28rpx;
  position: relative;
  top: 4rpx;
}

.addr {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  margin: 15rpx 0;
  font-size: 30rpx;
  color: #5b5b5b;
}

.distance {
   margin: 15rpx 0;
  font-size: 30rpx;
  color: #5b5b5b;
}

.time {
  font-size: 28rpx;
  color: #b1b1b1;
}

.tagList {
  width: 100%;
  overflow-x: scroll;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  margin-bottom: 5rpx;
  margin-top: 15rpx;
}

.tagList::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.tagItem {
  word-break: keep-all;
  font-size: 24rpx;
  padding: 2rpx 8rpx;
  margin-right: 15rpx;
  margin-top: 0rpx;
  box-sizing: border-box;
}

.bottom {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  margin-top: 15rpx;
}

.bottom-left {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}

.money {
  font-size: 34rpx;
  color: #ed1c1d;
}

.integral {
  font-size: 24rpx;
  color: #fe7860;
  border: 1rpx solid #fe7860;
  border-radius: 6rpx;
  padding: 2rpx 6rpx;
  height: 34rpx;
  margin-left: 15rpx;
  position: relative;
  top: 3rpx;
}

.bottom-right {
  display: flex;
  justify-content: flex-end;
  flex-direction: row;
  color: #aeaeae;
}

.go {
  width: 30rpx;
  height: 28rpx;
  position: relative;
  top: 5rpx;
}
