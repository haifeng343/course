page{
  padding-bottom: 120rpx;
}
.title {
  width: 100%;
  height: auto;
  padding: 0 5%;
  box-sizing: border-box;
  line-height: 50rpx;
  background-color: #fff;
  font-size: 32rpx;
}
.groupDetail{
  width: 100%;
  height: auto;
  padding: 0rpx 5%;
  background-color: #fff;
  box-sizing: border-box;
}
.groupDetail_title{
  font-size: 32rpx;
  color: #000;
  line-height: 60rpx;
}
.paycar{
  color: #26D7DF!important;
  border: 1rpx solid #26D7DF;
  background-color: #fff!important;
  margin-right: 20rpx!important;
}
.none{
  display: none!important;
}
.exList{
  padding:20rpx 0;
  background-color:#fff;
  margin-top: 20rpx
}
.groupDetail_content{
  font-size: 28rpx;
  color: #5b5b5b;
  line-height: 45rpx;
}
.courseList {
  background-color: #fff;
  width: 100%;
  height: auto;
  padding: 0 5%;
  box-sizing: border-box;
}
.courseList::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.banner {
  width: 100%;
  height: 285rpx;
  position: relative;
  z-index: 0;
  background-color: #fff;
}

swiper {
  height: 100%;
}

swiper-item {
  text-align: center;
}

.img1 {
  width: 96%;
  height: 100%;
  object-fit: cover;
  border-radius: 15rpx;
}

.box {
  width: 100%;
  height: 100%;
  display: inline-block;
  box-sizing: border-box;
}

.dot {
  position: absolute;
  bottom: 8rpx;
  right: 52rpx;
  background: rgba(0, 0, 0, 0.1);
  color: #c9c9c9;
  font-size: 24rpx;
  border-radius: 40rpx;
  padding: 3rpx 20rpx;
}

.content {
  background-color: #fff;
  width: 90%;
  padding: 30rpx 5% 10rpx;
}

.content .title {
  padding: 0;
  font-size: 32rpx;
  font-weight: 600;
}

.title .heng {
  width: 60rpx;
  height: 3rpx;
  background: #8d8d8d;
  position: relative;
  top: 22rpx;
}

.title .heng:first-child {
  margin-right: 30rpx;
}

.title .heng:last-child {
  margin-left: 30rpx;
}

.contents {
  display: flex;
  justify-content: space-between;
}

.money {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  font-size: 30rpx;
  color: #e51b1a;
}

.sym {
  font-size: 32rpx;
  position: relative;
  top: 7rpx;
}

.money .number {
  font-size: 32rpx;
  display: flex;
  align-items: flex-end;
}

.contents .time {
  font-size: 28rpx;
  line-height: 40rpx;
}


.courseItem {
  border-bottom: 1rpx solid #fbfbfb;
}
.courseItem .title {
  text-align: center;
  color: #5b5b5b;
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 10rpx 0 0;
}

.check {
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.courseCont {
  width: 100%;
  position: relative;
  height: auto;
  margin: 25rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  /* border-bottom: 1rpx solid #f7f7f7; */
  padding-bottom: 15rpx;
}
.courseCont:last-child{
  border: none;
}

.leftImg {
  width: 33%;
  height: auto;
  overflow: hidden;
  margin-right: 15rpx;
  border-radius: 10rpx;
}

.cover {
  width: 210rpx;
  height: 268rpx;
  border-radius: 15rpx;
}
.coverd{
  width: 136rpx;
  height: 136rpx;
  border-radius: 15rpx;
}
.right{
  font-size: 24rpx;
  color: #5b5b5b;
}
.coverc{
  width: 126rpx;
  height: 161rpx;
  border-radius: 15rpx;
}
.rightContent {
  width: 67%;
}

.courseTitle {
  width: 85%;
  font-size: 28rpx;
  font-weight: 600;
}

.coursePeople {
  font-size: 24rpx;
  color: #5b5b5b;
  line-height: 40rpx;
}

.tagList {
  width: 100%;
  overflow-x: scroll;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}

.tagList::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.relative {
  position: relative;
  line-height: 40rpx;
}
.absolute{
  position: absolute;
  right: 0;
  top:-2rpx;
  z-index: 1;
  display: flex;
  justify-content: flex-end;
}
radio .wx-radio-input.wx-radio-input-checked::before {
  position: absolute;
  top: 22rpx;
}

.tagItm {
  word-break: keep-all;
  font-size: 24rpx;
  padding: 2rpx 8rpx;
  margin-right: 15rpx;
  margin-top: 0rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
}

.detail {
  display: flex;
  justify-content: flex-start;
}

.detailImg {
  width: 34rpx;
  height:34rpx;
  position: relative;
  top: 4rpx;
  display: flex;
  right: 5rpx;
  justify-content: flex-start;
}

.name {
  font-size: 24rpx;
  color: #5b5b5b;
  line-height: 40rpx;
}

.addrLong {
  width: 100%;
  display: flex;
  justify-content: flex-start;
}

.long {
  width: 20%;
  text-align: right;
  color: #8d8d8d;
}

.around {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
}

.address {
  width: 90%;
  word-break: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #5b5b5b;
}

.ring {
  display: flex;
  justify-content: flex-start;
  margin-right: 25rpx;
}

.disclaimerList {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  background-color: #fff;
  margin-top: 15rpx;
}

.disTitle {
  font-size: 32rpx;
  font-weight: 700;
  padding-top: 30rpx;
  margin-left: 5%;
}

.disContent {
  width: 90%;
  font-size: 26rpx;
  line-height: 40rpx;
  margin-left: 5%;
  margin-top: 20rpx;
  padding-bottom: 30rpx;
}

.pay {
  font-size: 26rpx;
}

.again {
  color: #5b5b5b;
}

.el {
  height: 120rpx;
  display: flex;
  align-items: center;
}

.totalNumber {
  line-height: 120rpx;
  margin-right: 30rpx;
  font-size: 24rpx;
}
.kl{
  font-size: 24rpx;
}
.paybtn {
  background-color: #29d9d6;
  color: #fff;
  font-size: 32rpx;
  width: 200rpx;
  height:64rpx;
  line-height:64rpx;
  text-align: center;
  border-radius: 40rpx;
  margin:auto 0;
}

.kl .total {
  margin-top: 30rpx;
  color: #5b5b5b;
}

.payMoney {
  font-size: 32rpx;
  color: #e93a37;
}

.symbol {
  font-size: 24rpx;
}