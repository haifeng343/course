.Bill{
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  border-top: 1rpx solid #f9f9f9;
  border-bottom: 1rpx solid #f9f9f9;
}
.List{
  width: 100%;
  height: auto;
}
.item{
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 140rpx;
  line-height: 140rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}
.icon1{
  width: 40rpx;
  height: 34rpx;
  margin: 0 15rpx 0 0;
}
.go{
  margin: 0 0 0 15rpx;
}
.align{
  display: flex;
  align-items: center;
}
.bankIcon{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 15rpx;
}
.bankText{
  line-height: 40rpx;
}
.bankTitle{
  font-size: 28rpx;
  font-weight: 600;
}
.bankTxt{
  font-size: 26rpx;
  color: #5b5b5b;
}
.bankRight{
  margin-top: 55rpx;
}
.item1{
  width: 100%;
  height: 140rpx;
  box-sizing: border-box;
  padding: 0 5%;
  background-color: #fff;
}
.bor{
  border-top: 1rpx solid #f9f9f9;
}
.withdrawTxt{
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #5b5b5b;
}
.moneyTxt{
  font-size: 28rpx;
}
.moneyTxt:last-child{
  color: #3CD5D1;
}
.btn1{
  width: 95%;
  display: flex;
  align-items: center;
  margin: 0 auto;
  border-radius: 15rpx;
}
.start{
  position: relative;
}
.end{
  position: absolute;
  right: 0;
}
.money{
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
}
.item2{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #5b5b5b;
  box-sizing: border-box;
  padding: 0 5%;
  background-color: #fff;
  margin-bottom: 60rpx;
}
.g1{
  background-color: #CACACA;
}
.le{
  font-size: 50rpx;
  font-weight: 600;
  margin-top: 10rpx;
}
.btnSubmit{
  width: 95%;
  margin: 60rpx 0 40rpx;
  margin-left: 2.5%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #fff;
  background-color: #29D9D6;
  border-radius: 10rpx;
}
.success1{
  width: 100rpx;
  height: 100rpx;
  margin-top: 40rpx;
}
.le1{
  font-size: 32rpx;
  font-weight: 600;
}
.nohave{
  width: 100%;
  padding: 0 5%;
  box-sizing: border-box;
  font-size: 26rpx;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
}