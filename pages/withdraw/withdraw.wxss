.Bill {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 100rpx;
  font-size: 28rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  border-bottom: 1rpx solid #f9f9f9;
}

.Bill:last-child {
  border: none;
}

.List {
  width: 100%;
  height: auto;
}

.btnd {
  height: 60rpx;
}

.item {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 140rpx;
  line-height: 140rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}

.icon1 {
  width: 34rpx;
  height: 34rpx;
  margin: 0 15rpx 0 0;
}

.go {
  margin: 0 0 0 15rpx;
}

.align {
  display: flex;
  align-items: center;
}

.bankIcon {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 15rpx;
}

.bankText {
  line-height: 40rpx;
}

.bankTitle {
  font-size: 28rpx;
  font-weight: 600;
}

.bankTxt {
  font-size: 26rpx;
  color: #5b5b5b;
}

.mTxt {
  font-size: 32rpx;
  display: flex;
  align-items: center;
}

.item1 {
  width: 100%;
  height: 160rpx;
  box-sizing: border-box;
  padding: 0 5%;
  background-color: #fff;
}

.bor {
  border-top: 1rpx solid #f9f9f9;
}

.withdrawTxt {
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #5b5b5b;
}

.moneyTxt {
  font-size: 28rpx;
  color: #3dd6d1;
  margin-right: 20rpx;
  display: flex;
  align-items: center;
}

.btnd {
  width: 180rpx;
  margin-top: 0;
  font-size: 28rpx;
  line-height: 60rpx;
  border-radius: 40rpx;
}

.btn1 {
  width: 95%;
  display: flex;
  align-items: center;
  margin: 0 auto;
  border-radius: 15rpx;
}

.start {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.start_left {
  width: 60%;
  display: flex;
  justify-content: flex-start;
}

.betweens {
  display: flex;
  justify-content: space-between;
}

.start-middle {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.start-right {
  display: flex;
  justify-content: flex-end;
  flex-direction: row;
}

.clears {
  width: 28rpx;
  height: 28rpx;
}

.money {
  height: 80rpx;
  line-height: 80rpx;
  width: 80%;
  font-size: 32rpx;
  padding-right: 4%;
  box-sizing: border-box;
}

.item2 {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #5b5b5b;
  box-sizing: border-box;
  padding: 0 5%;
  background-color: #fff;
  margin-bottom: 60rpx;
}

.name {
  font-size: 28rpx;
  font-weight: bold;
  width: 75%;
  display: flex;
  line-height: 40rpx;
  justify-content: flex-start;
  align-items: center;
}

.g1 {
  color: #fff !important;
  background-image: linear-gradient(180deg, #cacaca, #cacaca) !important;
}

.le {
  font-size: 50rpx;
  font-weight: 600;
  margin-top: 10rpx;
}

.btnSubmit {
  width: 95%;
  margin: 60rpx 0 40rpx;
  margin-left: 2.5%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #fff;
  background-color: #29d9d6;
  border-radius: 10rpx;
}

.success1 {
  width: 100rpx;
  height: 100rpx;
  margin-top: 40rpx;
}

.le1 {
  font-size: 32rpx;
  font-weight: 600;
}

.List1 {
  width: 100%;
  height: auto;
  background-color: #fff;
}

.btndNo {
  background-color: #f6f6f6 !important;
  color: #ccc !important;
  background-image: none !important;
  border-radius: 40rpx;
  font-size: 28rpx;
}

.List1:last-child {
  margin-bottom: 20rpx;
}

.title {
  width: 90%;
  box-sizing: border-box;
  margin-left: 5%;
  padding: 25rpx 0;
  border-bottom: 1rpx solid #f4f4f4;
  display: flex;
  flex-direction: column;
}

.content {
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx 5%;
  display: flex;
  justify-content: flex-end;
}

.ml40 {
  margin-left: 40rpx;
}

.txt1 {
  width: 100%;
  line-height: 36rpx;
  font-size: 24rpx;
}

.titleText {
  font-size: 28rpx;
  font-weight: bold;
  width: 75%;
  line-height: 50rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.between {
  width: 100%;
}

.bb {
  font-size: 24rpx;
}

.mr20 {
  margin-right: 20rpx;
}

.text1 {
  border: 2rpx solid #f93736;
  box-sizing: border-box;
  width: 100%;
  height: 60rpx;
  padding: 0 20rpx;
  line-height: 60rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  text-align: center;
  border-radius: 40rpx;
  color: #f93736;
}

.btnd1 {
  width: 180rpx;
  margin: 0;
  height: auto;
  line-height: 60rpx;
  text-align: center;
  border: none;
  color: #fff;
  background-image: linear-gradient(180deg, #ff393a, #ff5e38);
}

.icon2 {
  width: 34rpx;
  height: 34rpx;
  margin-right: 15rpx;
}

.nohave {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 450rpx;
  display: flex;
  justify-content: center;
  border: 2rpx solid red;
  border-radius: 40rpx;
  text-align: center;
  box-sizing: border-box;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
}
