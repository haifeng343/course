<view class='classList'>
  <view class='classTitle'>西溪商圈</view>
  <view class='content-box'>
    <view class='classContent'>
      <view class='left'>
        <image src='https://file.guditech.com/Item/Main/20190718211938225_ec634635a64c4e46aab162a94a24212a.png' mode='apsectFill' class='leftImg'></image>
      </view>
      <view class='rightContent' data-Id="{{b.RelId}}" wx:key bindtap='courseDetail'>
        <view class='between relative'>
          <view class='courseTitle'>{{b.ItemName}}幼儿英语</view>
        </view>
        <view class='detail'>
          <view class='centers'>
            <image class='detailImg' src='../../images/house.png' mode='widthFix' lazy-load='true'></image>
          </view>
          <view class='name'>{{b.SellerName}}UUIDA教育</view>
        </view>
        <view class='detail around'>
          <view class='centers'>
            <image class='detailImg' src='../../images/ad1.png' mode='widthFix' lazy-load='true'></image>
          </view>
          <view class='name address'>{{b.StoreAddress}}西湖区三顿</view>
        </view>
        <view class='detail'>
          <view class='ring'>
            <view class='centers'>
              <image class='detailImg' src='../../images/couse.png' mode='widthFix' lazy-load='true'></image>
            </view>
            <view class='name'>共有{{b.CoursesNumber}}5节课</view>
          </view>
          <view class='ring'>
            <view class='centers'>
              <image class='detailImg' src='../../images/time.png' mode='widthFix' lazy-load='true'></image>
            </view>
            <view class='name'>{{b.SingleTime}} 60min/课时</view>
          </view>
        </view>
        <view class='tagList'>
          <view class='tagItm' wx:for="{{b.TagList}}" wx:key wx:for-item="l" style='background:{{l.TagBgColor}};color:{{l.TagFontColor}};border:2rpx solid {{l.BorColor}}'>{{l.TagName}}</view>
        </view>
        <view class='coursePeople'>{{b.SoldCount}}138人已参与 | 22{{b.RemainCount}}剩余名额</view>
      </view>
    </view>
  </view>
