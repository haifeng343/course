<view wx:if="{{BaseList.length>0}}">
  <view class='ruleList' wx:for="{{BaseList}}" wx:key>
    <view class="item {{item.IsExpire==true?'grop':''}}">
      <view class='between'>
        <view class='name'>
          <image src='../../images/cash.png' mode='aspectFill' class='cash'></image>
          <view class='title'>{{item.ReceiveName}}</view>
        </view>
        <view class='TaskFinishiStatus' wx:if="{{item.IsExpire==true}}">已过期</view>
        <view class='TaskFinishiStatus' wx:if="{{item.IsExpire==false}}"></view>
      </view>
      <view class='time'>领取时间:{{item.CreateTime}}</view>
      <view class='time'>有效日期:{{item.TaskEndTime}}</view>
      <view class='status mt10'>
        <view>奖励现金:
          <span class="red">￥{{item.PrizeAmountNow}}</span>
          (<span class wx:if="{{item.PrizeAmountNowStatus==1}}">未领取</span>
          <span wx:if="{{item.PrizeAmountNowStatus==2}}">领取中</span>
          <span wx:if="{{item.PrizeAmountNowStatus==3}}">已领取</span>
          <span wx:if="{{item.PrizeAmountNowStatus==4}}">领取失败</span>)</view>
        <view class='ml40'>奖励积分:
          <span class="red">{{item.PrizeScoreNow}}</span>(<span wx:if="{{item.PrizeScoreNowGot==true}}">已到账</span>
          <span wx:if="{{item.TaskFinishiStatus==false}}">未到账</span>)</ view>
        </view>
        <view class='status'>任务状态:
          <text>未完成</text>
        </view>
      </view>
    </view>
  </view>
  <view class='nohasLog' wx:else>
    <image class='noImg' src='../../images/zanwu.png' mode='widthFix' lazy-load='true'></image>
    <view>暂无现金券奖励记录</view>
  </view>

  <!--提现失败提示弹出框-->
  <view class="zan-dialog {{ showError ? 'zan-dialog--show' : '' }}" catchtouchmove="ture">
    <view class="zan-dialog__mask" bindtap="toggleDialog" />
    <view class="zan-dialog__container">
      <view class='zan-title'>失败原因</view>
      <view class='zan-content'>{{statusdes}}</view>
      <view class='know' bindtap='closed'>知道了</view>
    </view>
  </view>