page {
  background-color: white;
  height: 100vh;
}

.container {
  max-height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  padding: 20rpx 0rpx 100rpx;
}
.center{
  align-items: center;
}
.column{
  width: 100%;
}
.starts {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  width: 100%;
}

.content {
  width: 100%;
  padding: 10px;
  line-height: 22px;
  margin-right: 50rpx;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px;
}

.del {
  background-color: orangered;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.address {
  width: 100%;
  background-color: #fff;
  height: 100vh;
}

.noads {
  width: 450rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
}

.nohave {
  font-size: 32rpx;
  text-align: center;
  line-height: 50rpx;
}

.haveto {
  font-size: 28rpx;
  text-align: center;
}

.List {
  width: 100%;
  height: 100vh;
  padding: 0 0 0 5%;
  box-sizing: border-box;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  padding-bottom: 100rpx;
}

.item {
  width: 100%;
  box-sizing: border-box;
  padding: 25rpx 5%;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #f7f7f7;
}

.name {
  font-size: 28rpx;
  max-width: 80%;
}
.addressDetail {
  font-size: 24rpx;
  width: 80%;
  color: #a19b9b;
}

.edit {
  width: 34rpx;
  height: 34rpx;
}
