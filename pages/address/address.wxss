.address{
  width: 100%;
  background-color: #fff;
  height: 100vh;
}
.noads{
  width: 450rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
}
.nohave{
  font-size: 32rpx;
  text-align: center;
  line-height: 50rpx;
}
.haveto{
  font-size: 28rpx;
  text-align: center;
}
.add{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #42DFDE;
  font-size: 28rpx;
  position: fixed;
  bottom: 0;
  box-shadow: 0 0 15rpx #F9F9F9;
}
.List{
  width: 100%;
  height: auto;
  padding: 0 0 0 5%;
  box-sizing: border-box;
}
.item{
  width: 100%;
  padding: 25rpx 0;
  border-bottom:1rpx solid #F7F7F7;
}
.name{
  font-size: 32rpx;
}
.addressDetail{
  font-size: 26rpx;
  color: #A19B9B;
}
.edit{
  margin-right: 5%;
  width: 30rpx;
  margin-top: 35rpx;
}










