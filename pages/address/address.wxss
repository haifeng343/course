page {
  background-color: white;
}

.container {
  min-height: 100%;
  padding: 20rpx 0rpx 100rpx;
}

.touch-item {
  font-size: 14px;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #f9f9f9;
  width: 100%;
  overflow: hidden;
}

.content {
  width: 100%;
  padding: 10px;
  line-height: 22px;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px;
}

.del {
  background-color: orangered;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.address{
  width: 100%;
  background-color: #fff;
  height: 100vh;
}
.noads{
  width: 450rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
}
.nohave{
  font-size: 32rpx;
  text-align: center;
  line-height: 50rpx;
}
.haveto{
  font-size: 28rpx;
  text-align: center;
}
.List{
  width: 100%;
  height: 100vh;
  padding: 0 0 0 5%;
  box-sizing: border-box;
  overflow-y: scroll;
  padding-bottom: 100rpx;
}
.item{
  width: 100%;
  padding: 25rpx 0;
  border-bottom:1rpx solid #F7F7F7;
}
.name{
  font-size: 32rpx;
}
.addressDetail{
  font-size: 26rpx;
  color: #A19B9B;
}
.edit{
  margin-right: 5%;
  width: 30rpx;
  height: 24rpx;
  margin-top: 35rpx;
}