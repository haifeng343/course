
page{
  width: 100%;
  height: 100%;
}
.vipLog {
  width: 170rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #000;
  background: rgba(255, 255, 255, 0.4);
  font-size: 28rpx;
  position: fixed;
}
.bgImg{
  width: 100%;
  height: 100%;
  margin-bottom: 100rpx;
}
.centerTxt{
  z-index: 99;
  width: 90%;
  height: 600rpx;
  border-radius: 15rpx;
  background-color: #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  box-sizing: border-box;
  padding: 20rpx;
  word-break: break-all;
}