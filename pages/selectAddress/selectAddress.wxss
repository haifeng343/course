page{
  background-color: #fff;
}
.navTop{
  width: 100%;
  height: 90rpx;
  font-size: 28rpx;
  background-color: #fff;
  line-height: 90rpx;
  text-align: center;
}
.title{
  display: flex;
  justify-content: center;
  text-align: center;
  margin: 0 auto;
}
.navImg{
  width: 30rpx;
  padding-left: 15rpx;
margin-top:33rpx;
}
.topInput{
   background: #fff;
   height: 100rpx;
   line-height: 100rpx;
   position: relative;
}
.abso{
  position: absolute;
  font-size: 28rpx;
  left:10%;
  top:-15rpx;
  z-index: 999;
}
.abso::after{
  content: "";
  height: 30rpx;
  width: 4rpx;
  background: #D9D9D9;
  position: absolute;
  right: -15rpx;
  top: 35rpx;
}
.search{
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
  margin-left: 5%;
  border: 1px solid #F2F2F2;
  background: #F2F2F2;
  border-radius: 40rpx;
  background-image: url('data:image/png;base64,');
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
  background-position: 27% 50%;
}
.currentd{
  width: 100%;
  box-sizing: border-box;
  padding: 0 5% 15rpx;
  height: auto;
  background: #fff;
}
.kowd{
  margin: 15rpx 0;
}
.now{
  font-size: 26rpx;
  color: #959595;
}
.md{
  padding:20rpx 0 0 5%;
}
.again{
  font-size: 28rpx;
  color: #29D9D6;
}
.adIcon{
  width: 30rpx;
  margin-top: 8rpx;
  margin-right: 10rpx;
}
.addressDetail{
  font-size: 32rpx;
}
.about{
  width: 100%;
  background-color: #fff;
  height: auto;
}
.List{
  width: 100%;
  height: auto;
  padding: 0 0 0 5%;
  box-sizing: border-box;
}
.item{
  width: 95%;
  margin-left: 5%;
  padding: 25rpx 0;
  border-bottom:1rpx solid #F7F7F7;
}
.name{
  font-size: 32rpx;
}
.addressDetail{
  font-size: 26rpx;
  color: #A19B9B;
}
.addressList{
  width: 100%;
  max-height: 420rpx;
  overflow: hidden;
}
.addressItem{
  width: 90%;
  margin-left: 10%;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}














.add{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  position: fixed;
  border-radius: 10rpx;
  text-align: center;
  bottom: 40rpx;
  left: 5%;
  background: #29D9D6;
  color: #fff;
  font-size: 28rpx;
}














