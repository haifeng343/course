.navTop{
  width: 100%;
  height: 90rpx;
  font-size: 28rpx;
  background-color: #fff;
  line-height: 90rpx;
  text-align: center;
}
.title{
  display: flex;
  justify-content: center;
  text-align: center;
  margin: 0 auto;
}
.navImg{
  width: 30rpx;
  padding-left: 15rpx;
margin-top:33rpx;
}
.topInput{
   background: #fff;
   height: 100rpx;
   line-height: 100rpx;
   position: relative;
}
.abso{
  position: absolute;
  font-size: 28rpx;
  left:10%;
  top:-15rpx;
}
.abso::after{
  content: "";
  height: 30rpx;
  width: 4rpx;
  background: #D9D9D9;
  position: absolute;
  right: -15rpx;
  top: 35rpx;
}
.search{
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
  margin-left: 5%;
  border: 1px solid #F2F2F2;
  background: #F2F2F2;
  border-radius: 40rpx;
  background-image: url('data:image/png;base64,');
  background-size: 30rpx 30rpx;
  background-repeat: no-repeat;
  background-position: 27% 50%;
}

















