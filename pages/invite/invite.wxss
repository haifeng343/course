page {
  /* background-color: #fff; */
}

.item {
  width: 100%;
  box-sizing: border-box;
  padding: 0 5%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  font-size: 28rpx;
  position: fixed;
  top: 20rpx;
  z-index: 99;
}

.notPeople {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 28rpx;
  color: #6c6c6c;
}

.w100 {
  width: 100%;
}

.avator {
  width: 80rpx;
  border-radius: 50%;
  height: 80rpx;
  margin-right: 20rpx;
}

.itemLeft {
  width: 100%;
  margin-top: 10rpx;
}

.myTitle {
  width: 100%;
  box-sizing: border-box;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 5%;
  font-size: 24rpx;
  color: #5b5b5b;
  background-color: #fff;
  position: fixed;
  top: 120rpx;
  z-index: 99;
}

.logList {
  width: 100%;
  height: auto;
  background-color: #fff;
  font-size: 28rpx;
  margin-top: 200rpx;
}

.person {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.name {
  font-size: 28rpx;
}

.logItem {
  width: 100%;
  background-color: #fff;
  border-top: 1rpx solid #f9f9f9;
  border-bottom: 1rpx solid #f9f9f9;
  padding: 20rpx 5%;
  box-sizing: border-box;
}

.logItem:last-child {
  border: none;
}

.time {
  font-size: 24rpx;
  line-height: 35rpx;
  color: #717171;
 white-space: nowrap;
 overflow: hidden;
  text-overflow: ellipsis;
}
.w75{
  width: 75%;
}

.fraction {
  font-size: 28rpx;
}

.kepl {
  font-size: 28rpx;
  display: flex;
  align-items: center;
}
