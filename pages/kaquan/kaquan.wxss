.nav {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  display: flex;
  background-color: #fff;
  border-top: 1rpx solid #f4f4f4;
  justify-content: space-between;
  position: fixed;
  box-sizing: border-box;
  padding: 0 2.5%;
  top: 0;
  z-index: 1;
  align-items: center;
}

.navLeft {
  width: 92%;
  overflow: hidden;
  float: left;
}

.navLi {
  box-sizing: border-box;
  margin-left: 40rpx;
  white-space: nowrap;
  overflow: hidden;
}

.navLi:first-child {
  margin-left: 0;
}

.List {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  margin-top: 100rpx;
  margin-bottom: 20rpx;
}

.item {
  width: 95%;
  margin: 20rpx auto 0;
  background-color: #fff;
  box-sizing: border-box;
  border-radius: 15rpx;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;
}

.active {
  color: #3cd5cf;
}

.topImg {
  width: 8%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: -30rpx 0rpx 30rpx #fff;
}

.topImg image {
  width: 34rpx;
  height: 34rpx;
}

.allList {
  border-top: 1rpx solid #f4f4f4;
  width: 100%;
  height: auto;
  padding: 20rpx 3% 40rpx;
  position: absolute;
  top: 100rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  z-index: 2;
}

.allItem:nth-child(4n+1) {
  margin-left: 0;
}

.allItem.on {
  border-color: #3cd5cf;
  color: #3cd5cf;
}

.allItem {
  width: 22%;
  border: 1rpx solid #5b5b5b;
  margin-top: 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-left: 4%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.logo {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}

.title {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 5%;
  border-bottom: 1rpx solid #f4f4f4;
  margin-bottom: 20rpx;
}

.storeName {
  font-size: 28rpx;
  width: 83%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.content {
  width: 90%;
  display: flex;
  margin-left: 5%;
  flex-direction: column;
}

.quan {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  height: auto;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-attachment: cover;
  box-shadow: 5rpx 0rpx 10rpx #f7f7f7;
}

.up {
  width: 24rpx;
  height: 24rpx;
  margin-left: 10rpx;
}

.guoqi {
  position: absolute;
  width: 250rpx;
  text-align: center;
  background-color: #53e1de;
  color: #fff;
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
  transform: rotateZ(45deg);
  top: 30rpx;
  right: -71rpx;
}

.startd {
  display: flex;
  justify-content: flex-start;
}

.quanImg {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}

.quanContent {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 70%;
}

.noWrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.quanTitle {
  font-size: 32rpx;
}

.ft28 {
  font-size: 28rpx;
}

.foot {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}

.mt25 {
  margin-top: 25rpx;
}

.btnd {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 45rpx;
  margin-right: 0;
}

.dec {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 20rpx;
  color: #5b5b5b;
  font-size: 28rpx;
  box-shadow: 5rpx 5rpx 10rpx #f7f7f7;
}

.none {
  display: none;
}
