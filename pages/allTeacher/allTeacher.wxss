  background-color: #fff;
  padding: 30rpx 5%;
  box-sizing: border-box;
  display: flex;
  font-size: 24rpx;
}

.leftImg {
  width: 23%;
  margin-right: 15rpx;
}

.topImg {
  width: 150rpx;
  height: 128rpx;
  border-radius: 10rpx;
}

.rowImg {
  width: 32rpx;
  height: 24rpx;
  position: relative;
  top: 5rpx;
  right: 5rpx;
}
