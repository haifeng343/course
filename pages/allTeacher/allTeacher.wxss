.keep{
  width: 100%;
  height: auto;
  background-color: #fff;
  padding: 30rpx 5%;
  box-sizing: border-box;
  display: flex;
  font-size: 24rpx;
}
.leftImg{
  width:23%;
  margin-right: 15rpx;
}
.topImg{
  width: 150rpx;
  height: 128rpx;
  border-radius: 10rpx;
}
.rowImg{
  width: 40rpx;
  height: 40rpx;
}
.activity{
  font-size: 28rpx;
}
.right{
  width: 77%;
}
.h50{
  line-height: 45rpx;
}
.h40{
  line-height: 48rpx;
  display: flex;
  align-items: center;
}
.w100{
  width: 100%;
}
.address{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: keep-all;
}
.ml40{
  margin-left: 40rpx;
}
