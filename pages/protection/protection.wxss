.List{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
}
.item{
  width: 90%;
  height: auto;
  margin: 20rpx 5% 0;
  border-radius: 15rpx;
  background-color: #fff;
  box-sizing: border-box;
  padding: 15rpx 5%;
  font-size: 24rpx;
  line-height: 36rpx;
}
.itemTop{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
}
.title{
  width: 70%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
