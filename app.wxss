page {
  overflow-x: hidden;
  background-color: #eee;
}
page::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
image{
  will-change: transform;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.mt10{
  margin-top: 10rpx;
}
.clear{
  width: 40rpx;
  position: absolute;
  right: 15rpx;
  top:10rpx;
  z-index: 55;
}
.nohasLog{
  width: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  color: #6C6C6C;
  text-align: center;
}
.noImg{
  width: 400rpx;
  display: flex;
  justify-content: center;
  margin: 100rpx auto 0;
}
.loginBtnd{
  font-size: 28rpx;
  border: 1rpx solid #29D9D6;
  padding: 3rpx 25rpx;
  color: #29D9D6
}
.add{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #42DFDE;
  font-size: 28rpx;
  position: fixed;
  background-color: #fff;
  bottom: 0;
  box-shadow: 0 0 15rpx #F9F9F9;
}
.Top{
  width: 95%;
  height:auto;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 20rpx 2.5% 0;
  padding: 30rpx;
  box-sizing: border-box;
}
.List{
  font-size: 28rpx;
  color: #393939;
  height: 50rpx;
  line-height: 50rpx;
}
.red{
  color: red!important;
}
button::after {
  border: 0;
}
.line{
  height: 20rpx;
  width: 100%;
  background-color: #eee;
}
button {
  background-color: #fff;
  padding: 0;
}
.btn1{
  font-size: 28rpx;
  color: #fff;
  background-color: #29D9D6;
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
checkbox .wx-checkbox-input {
  height: 46rpx;
  width: 46rpx;
  border-radius: 50%;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 54rpx 54rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  height: 76rpx;
  width: 76rpx;
  color: transparent!important;
  border-radius: 50%;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 76rpx 76rpx;
}

.mt20{
  margin-top: 20rpx;
}
.center{
  display: flex;
  justify-content: center;
}
.home,.company{
  margin-left: 30rpx;
  color: #DC5B57;
  background-color: #FFF0F1;
  padding: 5rpx 15rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  margin-top: 6rpx;
}
.company{
  color: #80A5DC;
  background-color: #E6F9FE;
}
.around{
  display: flex;
  justify-content: space-around;
}
.button-hover {
  color: #000;
  background-color: #fff;
}
.flex-end{
  display: flex;
  justify-content: flex-end;
}
.column{
  display: flex;
  flex-direction: column;
}
.gray {
  color: #ccc !important;
  -webkit-filter: grayscale(80%);
  filter: grayscale(80%);
}

.between {
  display: flex;
  justify-content: space-between;
}

.icon {
  width: 30rpx;
}

.start {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}

.well {
  background-color: #fff;
  height: auto;
  padding: 0 5%;
}

.boxd {
  width: 100%;
  padding: 30rpx 5%;
  box-sizing: border-box;
  background-color: #fff;
}

.go {
  width: 30rpx;
}

.pay {
  position: fixed;
  z-index: 1;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  box-shadow: 0 -4rpx 10rpx #eee;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.dialog {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 99;
}

.dialog-contatner {
  position: fixed;
  margin: auto;
  background: #fff;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 80%;
  height: 350rpx;
  border-radius: 15rpx;
  padding: 40rpx 20rpx;
  box-sizing: border-box;
}

.zan-dialog__mask {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
  background: rgba(0, 0, 0, 0.6);
  display: none;
}

.zan-dialog__container {
  position: fixed;
  text-align: center;
  bottom: 400rpx;
  width: 650rpx;
  height: auto;
  margin-left: 50rpx;
  background: #fff;
  transform: translateY(300%);
  transition: all 0.4s ease;
  z-index: 12;
  border-radius: 20rpx;
}
.btnd{
  width: 300rpx;
  background-color: #29D9D6;
  height: 70rpx;
  font-size: 28rpx;
  color: #fff;
  margin-top: 25rpx;
}
.btnd1{
  color: #29D9D6;
  background: transparent;
  border:1rpx solid #29D9D6;
}
.know{
  font-size: 28rpx;
  color: #9FF6F0;
  border-top: 1rpx solid #E0E0E0;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 28rpx;
}
.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}
.zan-title{
  line-height: 100rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.zan-content{
  font-size: 28rpx;
  color: #434343;
  line-height: 40rpx;
}
.align-left{
  text-align: left;
}
.zan-til{
  text-align: left;
  font-weight: 600;
  font-size: 28rpx;
  line-height: 60rpx;
}
.le{
  padding: 0 50rpx;
}