page {
  overflow-x: hidden;
  background-color: #eee;
  font-family:'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.Top{
  width: 95%;
  height:auto;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 20rpx 2.5% 0;
  padding: 30rpx;
  box-sizing: border-box;
}
.List{
  font-size: 28rpx;
  color: #393939;
  height: 50rpx;
  line-height: 50rpx;
}
.red{
  color: red;
}
button::after {
  border: 0;
}

button {
  background-color: #fff;
  padding: 0;
}
.center{
  display: flex;
  justify-content: center;
}
.around{
  display: flex;
  justify-content: space-around;
}
.button-hover {
  color: #000;
  background-color: #fff;
}
.flex-end{
  display: flex;
  justify-content: flex-end;
}
.column{
  display: flex;
  flex-direction: column;
}
.gray {
  color: #ccc !important;
  -webkit-filter: grayscale(80%);
  filter: grayscale(80%);
}

.between {
  display: flex;
  justify-content: space-between;
}

.icon {
  width: 30rpx;
}

.start {
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}

.well {
  background-color: #fff;
  height: auto;
  padding: 0 5%;
}

.boxd {
  width: 100%;
  padding: 30rpx 5%;
  box-sizing: border-box;
  background-color: #fff;
}

.go {
  width: 30rpx;
}

radio .wx-radio-input {
  /* 自定义样式.... */
  height: 46rpx;
  width: 46rpx;
  border-radius: 50%;
  background: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: 54rpx 54rpx;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  width: 70rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  text-align: center;
  border-radius: 100%;
  background-color: #2bd8d7;
}

.pay {
  position: fixed;
  z-index: 1;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  box-shadow: 0 -4rpx 10rpx #eee;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.dialog {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 99;
}

.dialog-contatner {
  position: fixed;
  margin: auto;
  background: #fff;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 80%;
  height: 350rpx;
  border-radius: 15rpx;
  padding: 40rpx 20rpx;
  box-sizing: border-box;
}

.zan-dialog__mask {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
  background: rgba(0, 0, 0, 0.6);
  display: none;
}

.zan-dialog__container {
  position: fixed;
  text-align: center;
  bottom: 400rpx;
  width: 650rpx;
  height: auto;
  margin-left: 50rpx;
  background: #fff;
  transform: translateY(300%);
  transition: all 0.4s ease;
  z-index: 12;
  border-radius: 20rpx;
}
.btnd{
  width: 300rpx;
  background-color: #29D9D6;
  height: 70rpx;
  font-size: 28rpx;
  color: #fff;
  margin-top: 25rpx;
}
.btnd1{
  color: #29D9D6;
  background: transparent;
  border:1rpx solid #29D9D6;
}
.know{
  font-size: 28rpx;
  color: #9FF6F0;
  border-top: 1rpx solid #E0E0E0;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 28rpx;
}
.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}
.zan-title{
  line-height: 100rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.zan-content{
  font-size: 28rpx;
  color: #434343;
  line-height: 40rpx;
}
.align-left{
  text-align: left;
}
.zan-til{
  text-align: left;
  font-weight: 600;
  font-size: 28rpx;
  line-height: 60rpx;
}
.le{
  padding: 0 50rpx;
}